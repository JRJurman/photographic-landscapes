<html lang='en'>
	<head>
		{{> meta}}
		<title>Photographic Landscapes - Photos</title>
	</head>
	<style>
		section { display: grid; grid-template-columns: 1fr 1fr; } img { width: 100%; }
	</style>
	<body>
		<h1>Photos</h1>

		{{> nav}}
		<section>
			{{#each thumbnails}}
				<img src='{{this.fields.file.url}}' alt='{{this.fields.description}}' />
			{{/each}}
		</section>
	</body>
</html>
